package loteExercicio21;

import javax.swing.JOptionPane;

public class exercicio21 {
	public static void main(String args[]) {
		int nota1 = 0;
		int nota2 = 0;
		int nota3 = 0;
		int nota4 = 0;
		int media = 0;
		nota1 = Integer.parseInt(JOptionPane.showInputDialog(null,"insira a primeira nota"));
		nota2 = Integer.parseInt(JOptionPane.showInputDialog(null,"insira a segunda nota"));
		nota3 = Integer.parseInt(JOptionPane.showInputDialog(null,"insira a terceira nota"));
		nota4 = Integer.parseInt(JOptionPane.showInputDialog(null,"insira a quarta nota"));
		media = (nota1+nota2+nota3+nota4)/4;
		if(media>=6) {
			System.out.print("aprovado");
		}else if(media>=3) {
			System.out.print("exame");
		}else 
			System.out.print("retido");
		}
	}

